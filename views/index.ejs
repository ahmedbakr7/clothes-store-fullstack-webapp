<body>
  <!-- Navbar -->
  <%- include("./partials/navbar.ejs") %>

  <div class="video-container">
    <video autoplay loop muted id="video-bg">
      <source src="/images/Background Video.mp4" type="video/mp4" />
    </video>
    <div class="container-fluid d-flex flex-column justify-content-center align-items-center" style="height: 100%">
      <div>
        <h1 class="mainheader row justify-content-center">The North Face</h1>
      </div>
      <br />
      <div class="row">
        <h2 class="explorebtn" id="explorebtn">Explore</h2>
      </div>
    </div>
  </div>

  <!-- Best Selling Section -->
  <section class="pt-5 pb-5" id="bestsellingsection">
    <div class="container">
      <h2 class="row justify-content-center fw-bold">Best Selling</h2>
      <div class="row">
        <% if (products.length > 0) { %>
          <% products.forEach(product => { %>
            <% if(product.producttype=='tshirt' && product.gender=='men') {  %>
              <div class="col-lg-3 col-md-3 col-sm-6 productbox p-0 ">
                <a href="product"> <a href="/product/<%=product.title%>"> <img src="<%= product.imgsrc %>"
                      alt="Jacket 1" class="product-image"></a>
                  <h2 class="product-title"> <%= product.title %> </h2>
                  <span class="price"><%= product.price%></span>
                  <i class="bx bx-shopping-bag add-cart"></i>
              </div>
              <% } %>
          <% }) %>
        <% } else { %>
          <p>There are no products to display...</p>
        <% } %>


        <!-- <div class="col-lg-3 col-md-3 col-sm-6 productbox p-0"> -->

      </div>
    </div>
  </section>

